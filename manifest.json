{
  "name": "Fontty",
  "description": "Mark all fonts in this page in colors",
  "manifest_version": 2,
  "version": "1.0",
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "dist/dev/index.html"
  },
  "permissions": [
    "activeTab", 
    "tabs",
    "http://*/",
    "https://*/"
  ]
}